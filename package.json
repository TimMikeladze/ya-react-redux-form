{
  "name": "ya-react-form",
  "version": "0.0.1",
  "description": "Yet Another React Form",
  "main": "./lib/src/index.js",
  "typings": "./lib/src/index.d.ts",
  "scripts": {
    "compile": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c ./tslint.json -e test/fixtures/**/*.ts src/**/*.ts test/**/*.ts",
    "typings:install": "typings install",
    "testonly": "mocha lib/test/tests.js",
    "test": "npm run compile && npm run testonly",
    "posttest": "npm run lint",
    "prepublish": "rm -rf npm run compile",
    "postinstall": "typings install && npm dedupe"
  },
  "author": "Tim Mikeladze",
  "license": "MIT",
  "dependencies": {
    "object-assign-deep": "0.0.4",
    "react-redux": "^4.4.5",
    "react": "^15.0.2",
    "redux-thunk": "^2.0.1",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-spies": "^0.7.1",
    "mocha": "^2.5.3",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}

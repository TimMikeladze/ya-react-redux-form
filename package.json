{
  "name": "ya-react-form",
  "version": "0.0.1",
  "description": "Yet Another React Form",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint --ext .js --ext .jsx src/",
    "build": "babel src -d dist",
    "pretest": "npm run lint --silent",
    "test": "npm run build",
    "prepublish": "npm run build"
  },
  "author": "Tim Mikeladze",
  "license": "MIT",
  "dependencies": {
    "object-assign-deep": "0.0.4",
    "react": "^15.0.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "airbnb"
    ],
    "parser": "babel-eslint"
  }
}
